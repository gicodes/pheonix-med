
{
  "name": "pheonix_med",
  "version": "1.0.0",
  "description": "Root directory to Pheonix Meds",
  "main": "",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",

    "install": "npm-run-all --parallel install:backend install:frontend",

    "test": "echo \"Error: no test specified\" && exit 1",
    
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    
    "dev": "npm-run-all --parallel backend frontend",
    
    "build:backend": "cd backend && npx tsc",  
    "build:frontend": "cd frontend && npm run build",
    
    "build": "npm-run-all --parallel build:backend build:frontend"
  },
